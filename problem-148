#include <stdio.h>
#include <time.h>

int main() {
    time_t t = time(NULL);
    struct tm *gmt = gmtime(&t);

    printf("GMT Date & Time: %04d-%02d-%02d %02d:%02d:%02d\n",
        gmt->tm_year+1900, gmt->tm_mon+1, gmt->tm_mday,
        gmt->tm_hour, gmt->tm_min, gmt->tm_sec);

    return 0;
}

/*
Output (example):
GMT Date & Time: 2025-11-21 10:50:12
*/
